
{{
    Image( 123 )
    .size( 300, 250 )
    .timestamp()
    .img( {"data-origin" : "[[ this.size( 0 ).src() ]]", "alt" : "This image name [[ this.attr(name) ]], and..."} )
}}

{{
    Article( 123 )
    .image()
    .img()
}}

{{
    Article( 123 )
    .image()
    .src()
}}



{{
    Article( 123 )
    .image()
    .size( 300, 250 )
    .filename()
}}

{{
    Article( 123 )
    .attr( "name" )
}}

{{
    Article( 123 )
    .attr( "meta_keywords" )
}}



